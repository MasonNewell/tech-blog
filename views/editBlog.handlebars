<div class="card text-center">
  <div class="card-header">{{post_title}}</div>
  <div class="card-body">
    <div class="card-title">Posted by:
      {{user.name}}
      on
      {{date_created}}
    </div>
    <p class="card-text">
      {{post_contents}}
    </p>
    <button class="btn btn-secondary del-btn" id={{id}}>DELETE</button>
  </div>
</div>
<script src="/js/blog.js"></script>